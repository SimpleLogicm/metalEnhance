<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    xmlns:app="http://schemas.android.com/tools"
    android:layout_margin="5dp"
    card_view:cardCornerRadius="4dp">

    <RelativeLayout
        android:id="@+id/sub_h_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/counter_text_bg">

        <TextView
            android:id="@+id/d_name"
            android:layout_width="match_parent"
            android:layout_height="@dimen/card_title_height"
            android:background="#1D3A58"
            android:gravity="center_vertical"
            android:padding="@dimen/card_title_padding_top"
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:textSize="@dimen/card_title_text_size" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/d_name"
            android:layout_marginTop="0dp"
            android:paddingBottom="1dp"
            app:srcCompat="@drawable/rounded_corner">

            <TextView
                android:id="@+id/p_delaer_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:text="fghgfhgfh"
                android:textColor="@color/white"
                android:textSize="12dp"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/p_distance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/p_delaer_address"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:text=""
                android:textColor="@color/white"
                android:textSize="12dp" />


            <TextView
                android:id="@+id/p_proprietor_mobile1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/p_distance"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:text=""
                android:textColor="@color/white"
                android:textSize="12dp" />

            <TextView
                android:id="@+id/p_proprietor_email1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/p_proprietor_mobile1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:text=""
                android:textColor="@color/white"
                android:textSize="12dp"
                android:visibility="gone" />

            <TextView
                android:id="@+id/p_proprietor_gst_no"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/p_proprietor_email1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:visibility="gone"
                android:text=""
                android:textColor="@color/white"
                android:textSize="12dp"
                />

            <TextView
                android:id="@+id/p_stages"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/p_proprietor_gst_no"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:text=""
                android:textColor="@color/white"
                android:textSize="12dp"
                android:visibility="gone" />


            <TextView
                android:id="@+id/p_dealer_code"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:text=""
                android:textColor="@color/white"
                android:visibility="gone" />

            <TextView
                android:id="@+id/p_approved"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:text=""
                android:textColor="@color/white"
                android:visibility="gone" />

            <TextView
                android:id="@+id/p_lati"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:text=""
                android:textColor="@color/white"
                android:visibility="gone" />

            <TextView
                android:id="@+id/p_longi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:text=""
                android:textColor="@color/white"
                android:visibility="gone" />

            <TextView
                android:id="@+id/p_mobi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:text=""
                android:textColor="@color/white"
                android:visibility="gone" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="@dimen/common_5_dp"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="5dp">

                <Button
                    android:id="@+id/pro_mobile_click"
                    android:layout_width="80dp"
                    android:layout_height="50dp"

                    android:layout_marginRight="5dp"
                    android:background="@drawable/roundedbutton"
                    android:text="@string/Call"
                    android:textAllCaps="false"
                    android:textColor="@drawable/white_color"
                    android:textSize="17sp" />

                <Button
                    android:id="@+id/pro_check_distance"
                    android:layout_width="80dp"
                    android:layout_height="50dp"
                    android:layout_marginRight="5dp"
                    android:background="@drawable/roundedbutton"
                    android:text="@string/Shop_Distance"
                    android:textAllCaps="false"
                    android:textColor="@drawable/white_color"
                    android:textSize="17sp" />

                <Button
                    android:id="@+id/pro_click_order"
                    android:layout_width="80dp"
                    android:layout_height="50dp"
                    android:layout_marginRight="5dp"
                    android:background="@drawable/roundedbutton"
                    android:text="@string/Ordero"
                    android:textAllCaps="false"
                    android:textColor="@drawable/white_color"
                    android:textSize="17sp" />

                <Button
                    android:id="@+id/pro_click_edit"
                    android:layout_width="80dp"
                    android:layout_height="50dp"
                    android:layout_marginRight="5dp"
                    android:background="@drawable/roundedbutton"
                    android:text="@string/Edit"
                    android:textAllCaps="false"
                    android:textColor="@drawable/white_color"
                    android:textSize="17sp" />
            </LinearLayout>


        </RelativeLayout>
    </RelativeLayout>


</androidx.cardview.widget.CardView>
